generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  id        Int        @id @default(autoincrement())
  name      String
  createdAt DateTime   @default(now())
  Question  Question[]
}

model Question {
  id        Int      @id @default(autoincrement())
  question  String
  answer    Boolean
  creatorId Int
  createdAt DateTime @default(now())
  creator   User     @relation(fields: [creatorId], references: [id])
}
